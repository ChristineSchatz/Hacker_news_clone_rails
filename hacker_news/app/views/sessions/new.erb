<h2>Log in!</h2>
<% if flash[:errors] %>
  <div class="errors">
    <%= flash[:errors] %>
   </div>
   <br>
   <br>
<% end %>

<%=form_for(:session, url: login_path) do |f|%>
  <%=f.label :user_name%>
  <%=f.text_field :user_name%>
  <%=f.label :password%>
  <%=f.password_field :password%>
  <%=f.submit "Log In"%>
<%end%>
</br>

